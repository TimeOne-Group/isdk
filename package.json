{
  "name": "pbd-sdk",
  "version": "1.0.0",
  "description": "[![GitHub Super-Linter](https://github.com/TimeOne-Group/pbd-sdk/workflows/Lint%20Code%20Base/badge.svg)](https://github.com/marketplace/actions/super-linter)",
  "main": "index.js",
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "eslint": "eslint ./src/**",
    "test:browserstack": "node browserstack/index.mjs",
    "test:unit": "jest src",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test": "npm run test:unit && npm run test:browserstack",
    "predeploy": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimeOne-Group/pbd-sdk.git"
  },
  "author": "TimeOne <fabien.barbaud@timeonegroup.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/TimeOne-Group/pbd-sdk/issues"
  },
  "homepage": "https://github.com/TimeOne-Group/pbd-sdk#readme",
  "dependencies": {
    "@timeone-group/error-logger-js": "^0.2.3",
    "@timeone-group/storage-js": "^0.3.1",
    "core-js": "^3.9.1",
    "js-cookie": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.13.10",
    "@babel/register": "^7.13.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "babel-loader-exclude-node-modules-except": "^1.1.2",
    "browser-env": "^3.3.0",
    "browserstack-local": "^1.4.9",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "expect": "^28.0.2",
    "jest": "^28.0.3",
    "jest-environment-jsdom": "^28.0.2",
    "node-fetch": "^3.2.3",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.13",
    "prettier": "^2.6.1",
    "rollup": "^2.42.1",
    "rollup-plugin-cpy": "^2.0.1",
    "rollup-plugin-generate-html-template": "^1.7.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "selenium-webdriver": "^4.1.1",
    "sinon": "^13.0.2"
  },
  "browserslist": "last 2 versions, > 0.5%, ie >= 11",
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/*.test.mjs"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.mjs$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "mjs"
    ]
  }
}
