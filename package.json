{
  "name": "isdk",
  "version": "2.6.2",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "rollup -c -w",
    "build": "npm run clean && rollup -c",
    "build:prod": "npm run clean && NODE_ENV=production rollup -c",
    "clean": "rimraf dist debug browserstack/site",
    "lint": "eslint ./src/{**,__tests__/**}",
    "test:browserstack": "node browserstack/index.mjs",
    "test:unit": "npm run test:unit:path src",
    "test:unit:path": "NODE_ENV=test jest --runInBand --verbose",
    "test:unit:watch": "npm run test:unit:path src -- --watch",
    "test": "npm run test:unit && npm run test:browserstack",
    "release": "docker compose run test && standard-version -a -m 'chore(release): %s ðŸš€' --release-as minor",
    "hotfix": "docker compose run test && standard-version -a -m 'chore(hotfix): %s ðŸ”§' --release-as patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimeOne-Group/isdk.git"
  },
  "author": "TimeOne <fabien.barbaud@timeonegroup.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/TimeOne-Group/isdk/issues"
  },
  "homepage": "https://github.com/TimeOne-Group/isdk#readme",
  "dependencies": {
    "@timeone-group/storage-js": "^0.3.1",
    "core-js": "^3.36.0",
    "js-cookie": "^3.0.5",
    "lz-string": "^1.5.0",
    "regenerator-runtime": "^0.14.1",
    "uuid": "^9.0.1",
    "whatwg-fetch": "^3.6.20"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/register": "^7.23.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-loader-exclude-node-modules-except": "^1.2.1",
    "browser-env": "^2.0.21",
    "browserstack-local": "1.4.9",
    "dotenv": "^16.4.4",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "expect": "^29.7.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "node-fetch": "^3.3.2",
    "node-sass": "^9.0.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-generate-html-template": "^1.7.0",
    "rollup-plugin-postcss": "^4.0.2",
    "selenium-webdriver": "4.1.1",
    "standard-version": "^9.5.0"
  },
  "browserslist": "last 2 versions, > 0.5%, ie >= 11",
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.mjs"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.test.mjs"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.mjs$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "mjs"
    ]
  }
}
