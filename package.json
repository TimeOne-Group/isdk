{
  "name": "isdk",
  "version": "2.3.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "rollup -c -w",
    "build": "npm run clean && rollup -c",
    "build:prod": "npm run clean && NODE_ENV=production rollup -c",
    "clean": "rimraf dist debug browserstack/site",
    "lint": "eslint ./src/{**,__tests__/**}",
    "test:browserstack": "node browserstack/index.mjs",
    "test:unit": "npm run test:unit:path src",
    "test:unit:path": "NODE_ENV=test jest --runInBand --verbose",
    "test:unit:watch": "npm run test:unit:path src -- --watch",
    "test": "npm run test:unit && npm run test:browserstack",
    "release": "docker-compose run test && standard-version -a -m 'chore(release): %s ðŸš€' --release-as minor",
    "hotfix": "docker-compose run test && standard-version -a -m 'chore(hotfix): %s ðŸ”§' --release-as patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimeOne-Group/isdk.git"
  },
  "author": "TimeOne <fabien.barbaud@timeonegroup.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/TimeOne-Group/isdk/issues"
  },
  "homepage": "https://github.com/TimeOne-Group/isdk#readme",
  "dependencies": {
    "@timeone-group/storage-js": "^0.3.1",
    "core-js": "^3.9.1",
    "js-cookie": "^3.0.1",
    "lz-string": "^1.4.4",
    "regenerator-runtime": "^0.13.9",
    "standard-version": "^9.5.0",
    "uuid": "^8.3.2",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.13.10",
    "@babel/register": "^7.13.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^4.0.0",
    "babel-loader-exclude-node-modules-except": "^1.1.2",
    "browser-env": "^3.3.0",
    "browserstack-local": "^1.4.9",
    "dotenv": "^16.0.1",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "expect": "^28.0.2",
    "husky": "^8.0.1",
    "jest": "^28.0.3",
    "jest-environment-jsdom": "^28.0.2",
    "jest-fetch-mock": "^3.0.3",
    "node-fetch": "^3.2.3",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.13",
    "prettier": "^2.6.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.42.1",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-cpy": "^2.0.1",
    "rollup-plugin-generate-html-template": "^1.7.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "selenium-webdriver": "^4.1.1"
  },
  "browserslist": "last 2 versions, > 0.5%, ie >= 11",
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.mjs"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.test.mjs"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.mjs$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "mjs"
    ],
    "standard-version": {
      "scripts": {
        "postbump": "npm run build:prod"
      }
    }
  }
}
